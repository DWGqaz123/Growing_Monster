.monster-gallery {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  padding: 20rpx;
  margin-top: 100rpx;
  height: 500rpx;
}
.monster-image {
  width: 150rpx;
  height: 150rpx;
  margin-bottom: 20rpx;
}
.title{
  font-size: 40px;
  text-align: center;
  width: 50%;
  margin-top: 150rpx;
  margin-right: 185rpx;
  background-color: rgb(85, 211, 173);
  border-radius: 30px;
}
.top{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.back{
  width: 10%;
}